{
    "dockerComposeFile": "docker-compose.yml",
    "service": "cli",
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
    "remoteUser": "vscode",
    "customizations": {
        "vscode": {
            "extensions": [
                "bmewburn.vscode-intelephense-client"
            ],
            "settings": {
                "intelephense.stubs": [
                    "Core",
                    "ctype",
                    "curl",
                    "date",
                    "dom",
                    "fileinfo",
                    "filter",
                    "gd",
                    "hash",
                    "iconv",
                    "intl",
                    "json",
                    "libxml",
                    "mbstring",
                    "mysql",
                    "openssl",
                    "pcre",
                    "PDO",
                    "pdo_mysql",
                    "pdo_sqlite",
                    "Phar",
                    "posix",
                    "random",
                    "readline",
                    "redis",
                    "Reflection",
                    "session",
                    "SimpleXML",
                    "sodium",
                    "SPL",
                    "sqlite3",
                    "standard",
                    "tokenizer",
                    "xdebug",
                    "xml",
                    "xmlreader",
                    "xmlwriter",
                    "zlib"
                ]
            }
        }
    }
}
